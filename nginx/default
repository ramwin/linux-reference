server {
    listen 80;
    listen [::]:80;

    root /var/www/html;

    index index.html index.htm index.nginx-debian.html;

    server_name www.ramwin.com;

    location / {
        try_files $uri $uri/ =404;
        charset UTF-8;
        charset_types *;
        limit_rate 1m;
        gzip_static on;
    }
}
